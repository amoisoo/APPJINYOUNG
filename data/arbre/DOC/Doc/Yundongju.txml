    <Record Index="105" ParentIndex="103" Expand="0">
        <TITLE color="None" background="None">투르게네프의 언던</TITLE>
        <ID>55</ID>
        <ID_SORT>105</ID_SORT>
        <DEPTH>103</DEPTH>
        <PROTECTION></PROTECTION>
